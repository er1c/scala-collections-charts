  Buffer -> ListBuffer
  mSeq -> ArrayStack
  mIterable -> mSet
  mSet -> mHashSet [penwidth=3]
  mSet -> mBitSet
  mSet -> LinkedHashSet
  mSet -> ImmutableSetAdaptor
  mIterable -> mMap
  mMap -> mHashMap [penwidth=3]
